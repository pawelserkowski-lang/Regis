{
  "name": "cyber-deck-protocol",
  "version": "27.5.1",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "postinstall": "cd electron/jules-mcp-server && npm install",
    "build": "cd electron/jules-mcp-server && npm run build && cd ../.. && tsc && vite build && electron-builder"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.1",
    "@monaco-editor/react": "^4.6.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.447.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.8",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.9.3",
    "vite": "^5.4.8",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "extraResources": [
      {
        "from": "electron/jules-mcp-server",
        "to": "electron/jules-mcp-server",
        "filter": [
          "**/*",
          "!**/src/**",
          "!**/tsconfig.json",
          "!**/vitest.config.ts"
        ]
      }
    ]
  }
}
